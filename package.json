    "socket.io-client": "^2.4.0",
    "wif": "^2.0.6"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.17.58",
    "@types/sinon": "^7.5.2",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.0.1",
    "nock": "^10.0.6",
    "node-mocks-http": "^1.10.1",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "semantic-release": "^15.14.0",
    "sinon": "^4.5.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  }
