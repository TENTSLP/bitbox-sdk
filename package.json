{
  "name": "bitbox-sdk",
  "version": "8.11.0",
  "description": "BITBOX SDK for Bitcoin Cash",
  "author": "Gabriel Cardona <gabriel@bitcoin.com>",
  "contributors": [
    "Chris Troutner <chris.troutner@gmail.com>"
  ],
  "scripts": {
    "test": "npm run test:unit",
    "test:no-build": "nyc mocha test/unit --timeout 60000 --exit",
    "test:unit": "tsc && nyc mocha test/unit --timeout 60000 --exit",
    "test:integration": "TEST=integration nyc --reporter=text mocha --timeout 30000 test/integration --exit",
    "test:integration:local": "TEST=integration SERVER=local nyc --reporter=text mocha --timeout 30000 test/integration --exit",
    "test:integration:stage": "TEST=integration SERVER=stage nyc --reporter=text mocha --timeout 30000 test/integration --exit",
    "test:integration:custom": "TEST=integration SERVER=custom mocha --timeout 30000 test/integration --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "tsc"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.15.3"
  },
  "main": "index.js",
  "files": [
    "index.ts",
    "lib/"
  ],
  "homepage": "https://developer.bitcoin.com/bitbox",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bitcoin-com/bitbox-sdk.git"
  },
  "dependencies": {
    "@bitcoin-dot-com/bitcoincashjs2-lib": "ciripel/bitgo-utxo-lib",
    "@types/bigi": "^1.4.2",
    "@types/bip39": "^2.4.2",
    "@types/randombytes": "^2.0.0",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "@types/wif": "^2.0.2",
    "assert": "^1.4.1",
    "axios": "0.19.0",
    "bc-bip68": "^1.0.5",
    "bigi": "^1.4.2",
    "bip-schnorr": "^0.3.0",
    "bip21": "TENTSLP/bip21",
    "bip32-utils": "TENTSLP/bip32-utils#0.13.1",
    "bip38": "^2.0.2",
    "bip39": "^2.5.0",
    "bip66": "^1.1.5",
    "bitcoincash-ops": "TENTSLP/bitcoincash-ops#2.0.0",
    "bitcoinjs-message": "TENTSLP/bitcoinjs-message#v2.1.0",
    "bs58": "^4.0.1",
    "cashaddrjs": "TENTSLP/cashaddrjs",
    "eventsource": "^1.1.0",
    "randombytes": "^2.0.6",
    "safe-buffer": "^5.2.1",
    "satoshi-bitcoin": "^1.0.4",
